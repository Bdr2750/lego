{
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "methods": ["GET"]
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Content-Security-Policy", "value": "default-src 'self' 'unsafe-inline' https://*.vercel.app" }
      ]
    }
  ]
}